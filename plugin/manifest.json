{
  "manifest_version": 2,

  "name": "OmniWheel",
  "description": "JIRA Assistant",
  "version": "1.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "audioCapture",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "<all_urls>",
    "notifications"
  ]
}